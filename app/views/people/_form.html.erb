<%= form_with(model: person) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :age %>
    <%= form.number_field :age, min: 0, max: 99, step: 1 %>
  </div>
  <div>
    <%= form.label :mood %>
    <%= form.text_field :mood %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>